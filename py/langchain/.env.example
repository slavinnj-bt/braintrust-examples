# API Keys (at least one required)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
MISTRAL_API_KEY=your_mistral_key_here

# Braintrust
BRAINTRUST_API_KEY=your_braintrust_key_here

# Optional: Set default provider and model via environment
# These can be overridden via Braintrust UI parameters
AI_PROVIDER=openai  # or anthropic, or mistral
AI_MODEL=gpt-4o-mini  # or claude-sonnet-4-5-20250929, or mistral-small-latest

# Modal Deployment Configuration
# Set to 'modal' to use Modal cloud deployment, 'local' for localhost
#EVAL_DEPLOYMENT=local
#MODAL_ENDPOINT_URL=
